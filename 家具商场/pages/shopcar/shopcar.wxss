.trash{
  width: 100%;
  height: 7vh;
  background: #fff;
  margin-bottom: 15rpx;
}
.trash text{
  height: 7vh;
  line-height: 7vh;
  color: #F5B223;
  margin-left: 10rpx;
}
.trash image{
  width: 70rpx;
  height: 70rpx;
  float: right;
  margin-top: 8rpx;
  margin-right: 8rpx;
}
.sclist{
  width: 100vw;
  height: 20vh;
  background: #fff;
  margin-top: 10rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}

.sclist image{
  width: 200rpx;
  height: 190rpx;
}
.sclist>view{
  width: 430rpx;
  height: 200rpx;
  
  padding: 0 2%;
}
.sclist .title{
  width: 100%;
  height: 40%;
  
  font-size: 14px;
  line-height: 40rpx;
  margin-top: 10rpx;
}
.sclist .detail{
  width: 100%;
  height: 50%;
  
  display: flex;
}
.detail .price{
  width: 40%;
  height: 100%;
  line-height: 100rpx;
  color: rgba(245, 164, 11, 1);
}
.detail .count{
  width: 60%;
  height: 100%;
  line-height: 100rpx;
  color: rgba(245, 164, 11, 1);
  
  display: flex;
  justify-content: center;
  align-items: center;
}
.count view{
  width: 60rpx;
  height: 60rpx;
}
.count view image{
  width: 100%;
  height: 100%;
  
}
.count input{
  width: 70rpx;
  height: 60rpx;
  text-align: center;
}
.checkedall{
  width: 100%;
  height: 8vh;
  position: fixed;
  bottom: 0vh;
  background: #fff;
  display: flex;
  justify-content: space-around;
  align-items: center;
  font-size: 16px;
}
.checkedall checkbox-group{
  width: 20%;
  font-size: 14px;
  text-align: center;
  vertical-align: center;
}
.money{
  width: 58%;
  text-align: left;
  box-sizing: border-box;
  padding-left:2%; 
}
.money text{
  color: red;
}
.pay{
  width:22%;
  height: 100%;
  line-height: 8vh;
  font-size: 16px;
  text-align: center;
  color: #fff;
  background: #F5B223;
}
